#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int lit, price;
    scanf("%d", &lit);

    if (lit<=10){
        price = 7;
        printf("%d\n", price);
    }
    else if(lit>10 && lit<=30){
        price = 7 + 1*(lit-10);
        printf("%d\n", price);
    }
    else if(lit>30 && lit<=100){
    price = 7 + 20 + 2*(lit-30);
    printf("%d\n", price);
    }
    else{
        price = 27+140+5*(lit-100);
        printf("%d\n", price);
    }
	return 0;
}
